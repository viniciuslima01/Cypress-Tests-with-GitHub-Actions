{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Cypress Run Debug",
      "program": "${workspaceFolder}/node_modules/cypress/bin/cypress",
      "args": ["run"],
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["--inspect-brk"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
